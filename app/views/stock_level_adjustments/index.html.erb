<p id="notice"><%= notice %></p>

<h1>Listing Stock Level Adjustments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stock_level_adjustments.each do |stock_level_adjustment| %>
      <tr>
        <td><%= link_to 'Show', stock_level_adjustment %></td>
        <td><%= link_to 'Edit', edit_stock_level_adjustment_path(stock_level_adjustment) %></td>
        <td><%= link_to 'Destroy', stock_level_adjustment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stock level adjustment', new_stock_level_adjustment_path %>
