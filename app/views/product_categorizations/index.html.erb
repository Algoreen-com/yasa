<p id="notice"><%= notice %></p>

<h1>Listing Product Categorizations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_categorizations.each do |product_categorization| %>
      <tr>
        <td><%= link_to 'Show', product_categorization %></td>
        <td><%= link_to 'Edit', edit_product_categorization_path(product_categorization) %></td>
        <td><%= link_to 'Destroy', product_categorization, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product categorization', new_product_categorization_path %>
